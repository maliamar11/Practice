pipeline {
  agent any
  stages {
    stage('Checkout') {
      steps {
        echo 'echo \'Hello this is checkout step\''
      }
    }

    stage('Build') {
      steps {
        echo 'echo \'Building the project\''
      }
    }

    stage('Achiving') {
      steps {
        echo 'echo \'Archiving the reports\''
      }
    }

  }
}